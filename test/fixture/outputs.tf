/* network_test1 outputs **/


output "virtual_network_address_space1" {
  description = "List of address spaces that are used the virtual network."
  value       = module.network_test1.virtual_network_address_space
}

output "virtual_network_name1" {
  description = "The name of the virtual network"
  value       = module.network_test1.virtual_network_name
}

output "virtual_network_id1" {
  description = "The id of the virtual network"
  value       = module.network_test1.virtual_network_id
}

output "subnet_ids1" {
  description = "List of IDs of subnets"
  value       = module.network_test1.subnet_ids
}
  
output "subnet_names1" {
  description = "List of names of subnets"
  value       = module.network_test1.subnet_names
}

output "network_security_group_ids1" {
  description = "List of the Network security groups and ids"
  value       = module.network_test1.network_security_group_ids
}

